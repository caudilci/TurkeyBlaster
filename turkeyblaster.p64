--[[pod_format="raw",created="2025-07-06 21:57:03",modified="2025-07-12 05:23:57",revision=1019]]
include("utils.lua")
include("enemies.lua")
include("bullets.lua")
include("player.lua")
include("expgems.lua")

function _init()
	gamestates = {"title","level", "play", "death"}
	init_player()
	init_bullets()
	init_enemies()
	init_expgems()
	animations = {
		gunfire = {
			sprites = {}
		}
	}
	frame = 0
	mouse_enabled = true
	vid()
	fetch("pal/0.pal"):poke(0x5000)
	poke(0x5f58, 0x1 | 0x2 | 0x4 | 0x8)
end

function _update()
	update_player()
	update_bullets()
	update_enemies()
	
	-- frame	
	frame += 1
end

function _draw()
	cls()
	map(1)
	draw_bullets()
	draw_enemies()
	-- this needs to be last because of interactions with tline3d
	draw_player()
end


